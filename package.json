{
  "name": "clickhouse-github-log-importer",
  "version": "1.0.0",
  "description": "GitHub Log Importer for Clickhouse",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --max_old_space_size=8192 --daemon --title=egg-server-ghLogMgr",
    "stop": "egg-scripts stop --title=egg-server-ghLogMgr",
    "dev": "egg-bin --max_old_space_size=8192 dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "@apla/clickhouse": "^1.6.3",
    "dateformat": "^3.0.3",
    "egg": "^2.6.1",
    "egg-scripts": "^2.6.0",
    "node-worker-threads-pool": "^1.3.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^14.0.1",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "egg-ts-helper": "^1.25.8",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "tslib": "^1.13.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "frank-zsy",
  "license": "MIT"
}
